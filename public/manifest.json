{
  "manifest_version": 3,
  "name": "TicTab",
  "description": "On a new page, current or in a new tab",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contents/content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"]
}
